{
    use esmith::AccountsDB;
    # Convert the passed hash for the ibay object back into an object.
    $ibay = bless \%ibay, 'esmith::DB::db::Record';

    $key = $ibay->key;
    $OUT .= "\n[$key]\n";
    my $comment = $ibay->prop('Name') || "";
    $OUT .= "comment = $comment";

    $ibay_vfs = ();
}
