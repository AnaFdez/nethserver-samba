# 20profile_default:
read only            = no
inherit permissions  = yes
create mask          = 0664 ; Add group write bit to default create mask, remove DOS archive bit (see below) #2039
inherit owner        = yes
store dos attributes = yes  ; Use extended attribute to store DOS attributes (see man page)
map archive          = no
map readonly         = no
inherit acls         = yes
map acl inherit      = yes
guest ok             = { ($ibay{SmbGuestAccessType} || 'none') =~ /^rw?$/ ? 'yes' : 'no' }
